<?language javascript?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.HBox?>
<HBox fx:controller="ui.NavigateBarController"
      xmlns:fx="http://javafx.com/fxml" alignment="TOP_LEFT" spacing="10">
    <padding><Insets top="5" right="5" bottom="10" left="5"/></padding>
    <children>
        <Button onAction="#backClick">
            <graphic>
                <ImageView fitHeight="24" fitWidth="24" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="/ui/resources/images/back.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <FlowPane hgap="10" vgap="10" alignment="CENTER_LEFT" fx:id="breadCrumbsPane" HBox.hgrow="ALWAYS"/>
    </children>
</HBox>