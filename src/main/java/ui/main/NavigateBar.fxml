<?language javascript?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox fx:controller="ui.main.NavigateBarController"
      xmlns:fx="http://javafx.com/fxml" alignment="center">
    <padding><Insets top="5" right="5" bottom="10" left="5"/></padding>
    <children>
        <Text fx:id="infoText"/>
        <FlowPane hgap="10" vgap="10" alignment="CENTER_LEFT">
            <Button onAction="#homeClick">
                <graphic>
                    <ImageView fitHeight="24" fitWidth="24" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="/ui/main/resources/home.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
            <Button onAction="#backClick">
                <graphic>
                    <ImageView fitHeight="24" fitWidth="24" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="/ui/main/resources/back.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
        </FlowPane>
    </children>
</VBox>